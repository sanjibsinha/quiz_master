
import 'dart:math';
void main() {
  
  int maximum = 20;
  
  int minimun = 10;
  
  print(getMaximum(maximum, minimun)); // 20
  
  print(getMinimum(maximum, minimun)); // 10
  
  print(max(maximum, minimun));
  
  print(min(maximum, minimun));
  
  
}

int getMaximum(int a, int b){
    if(a < b){
        return b;
    } else {
        return a;
    }
 }

    int getMinimum(int a, int b){
    if(a > b){
        return b;
    } else {
        return a;
    }
 }
